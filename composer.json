{
  "name": "duncrow-gmbh/captcha-eu",
  "version": "1.0.1",
  "description": "Captcha EU Extension for Contao Open Source CMS",
  "keywords": [
    "contao",
    "captcha-eu",
    "duncrow gmbh"
  ],
  "type": "contao-bundle",
  "homepage": "https://duncrow.com",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Duncrow GmbH",
      "email": "office@duncrow.com",
      "homepage": "https://duncrow.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "contao/core-bundle": "^4.13 || ^5.3"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "DuncrowGmbh\\CaptchaEu\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "files": [],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "DuncrowGmbh\\CaptchaEu\\ContaoManager\\Plugin"
  }
}
